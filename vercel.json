{
    "builds":[
        {
            "src": "./app/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "app/pages/**/*",
            "use": "@vercel/static"
          }
    ],
    
    "routes":[
        {
            "src": "/(.*)",
            "dest": "app/index.js"
        }
    ]
}